{
    "version": 2,
    "builds": [
        {
            "src": "client/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/client/build/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/client/build/index.html"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "DEMO_MODE": "true"
    }
}